{
  "name": "backend-cli",
  "version": "1.0.0",
  "description": "Backend Command-Line Interface with IPC support",
  "main": "dist/cli.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:calculate": "node dist/cli.js calculate \"{\\\"operation\\\":\\\"add\\\",\\\"a\\\":5,\\\"b\\\":3}\"",
    "test:help": "node dist/cli.js help",
    "test:help:usage": "node dist/cli.js",
    "scan:test": "tsc -p tsconfig.scantest.json && node dist/__tests__/scanme.js"
  },
  "keywords": [
    "cli",
    "command-line",
    "ipc",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
