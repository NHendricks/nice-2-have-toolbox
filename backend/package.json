    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.33",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/image-size": "^0.7.0",
    "@types/pdfkit": "^0.17.5",
    "adm-zip": "^0.5.16",
    "basic-ftp": "^5.1.0",
    "compromise": "^14.14.5",
    "fast-xml-parser": "^5.3.7",
    "image-size": "^2.0.2",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.2",
    "tesseract.js": "^7.0.0"
  },
  "overrides": {
    "minimatch": "^10.2.2",
    "glob": "^13.0.6"
  }
